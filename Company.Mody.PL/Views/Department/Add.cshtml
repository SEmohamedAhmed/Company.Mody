@model Department


@{
    ViewData["Title"] = "Add";
}

<h1>Create Department</h1>

<br />
<br />


<form action="Create" method="post">
    
    <partial name="PartialViews\_AddUpdatePartial.cshtml" />

    <br />
    <input asp-action="Add" type="submit" value="Create" class="btn btn-primary"/>
    <a asp-action="Index" class="btn btn-light" >Back to departments list</a>

</form>

@section JQueryValidation {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

}