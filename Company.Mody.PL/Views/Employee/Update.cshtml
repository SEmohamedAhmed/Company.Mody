@model Employee
@{
    ViewData["Title"] = "Update";
}

<h1>Update Employee</h1>



<br />
<br />


<form  method="post">
    

    <partial name="\PartialViews\_AddUpdatePartial.cshtml" @*model="Model"*@ />

    <br />

    <input asp-action="Update" type="submit" value="Update" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back to employees list</a>

</form>

@section JQueryValidation {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

}