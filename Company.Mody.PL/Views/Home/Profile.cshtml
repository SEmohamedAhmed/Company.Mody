@model ProfileUser
@{
    Layout = "_Layout";
    ViewData["Title"] = "Profile";
}


<h1>Your Profile</h1>


<div>
    <div class="form-group">
        <label asp-for="UserName" class="col-form-label">User Name</label>
        <input asp-for="UserName" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label asp-for="FirstName" class="col-form-label">First Name</label>
        <input asp-for="FirstName" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label asp-for="LastName" class="col-form-label">Last Name</label>
        <input asp-for="LastName" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber" class="col-form-label">Phone Number</label>
        <input asp-for="PhoneNumber" class="form-control" readonly />

    </div>
    <div class="form-group">
        <label asp-for="Email" class="col-form-label">Email</label>
        <input asp-for="Email" class="form-control" readonly />

    </div>

</div> 